{% if visualisations | length === 1 %}
  {{ comp.viz.render(visualisations[0]) | safe }}
{% elif visualisations | length > 1 %}

  <div class="panes" role="tabpanel">
  {% for vis in visualisations %}
	<div class="pane">
	{{ comp.viz.render(vis) | safe }}
	</div>
  {% endfor %}
  
  </div>

{% endif %}

