<ul>
  {% for dashboard in search.pages("dashboard", "title") | data %}
  <li>
    <a href="{{ dashboard.url }}">{{ dashboard.title }}</a>
  </li>
  {% endfor %}
</ul>

{# HEADLINES ACROSS TOP #}
{% comp 'grid.auto' %}
  {% comp 'grid.pane' %}
    <h3>Most recent long-term unemployed</h3>
    <p>{{ sources.qlfs.long_term_unemployed.columns.age_16_to_24_unemployed_over_12_months_rate_sa | last }}</p>
  {% endcomp %}
  {% comp 'grid.pane' %}
    <h3>QLFS visualisation</h3>
    <p>{{ comp.viz.main({
      visualisations: search.data('dashboard/qlfs/index.njk').visualisations[1]
    }) | safe }}</p>
  {% endcomp %}
{% endcomp %}