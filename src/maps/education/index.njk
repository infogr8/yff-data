    <article id='attainment-score-total' class='selector-block' data-selector-group="Education Attainment">
    <h2>Attainment 8 Scores - Total</h2>
    {% comp 'display.figure' %}
      {{ comp.oi.map.hex_cartogram({
        config: {
          hexjson: uk_local_authority_districts_2021,
          data: view.ks4_outcomes.rows,
          matchKey: 'LADCD',
          value: 'avg_att8→Total',
          scale: 'YFF_r',
		  min: 30,
		  max: 60,
		  legend: {
			position: 'top left',
			show: true,
			items:[{
				value: 60,
				label: ">60"
			},{
				value: 50,
				label: "50"
			},{
				value: 40,
				label: "40"
			},{
				value: 30,
				label: "<30"
			}]
		  },
          tooltip: '<strong>{{ n }}</strong>\nAttainment 8\nTotal: <strong>{{ avg_att8→Total | toFixed(1) }}</strong>\nGirls: <strong>{{ avg_att8→Girls | toFixed(1) }}</strong>\nBoys: <strong>{{ avg_att8→Boys | toFixed(1) }}</strong>'
        } 
      }) | safe }}
    {% endcomp %}
    </article>



