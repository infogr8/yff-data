    <article id='total-score' class='selector-block' data-selector-group="Aggregate scores">
    <h2>Total Score</h2>
    {{ chart_description | md | safe }}
    </br>
    {% comp 'display.figure' %}
      {{ comp.oi.map.hex_cartogram({
        config: {
          hexjson: uk_local_authority_districts_2021,
          data: view.total_score.rows,
          matchKey: 'local_authority_code',
          value: 'value',
          scale: 'YFF',
          tooltip: '{{ n }}: <strong>{{ value | toFixed(2) }}</strong>'
        } | autoLegend({ formatter: legendKey })
      }) | safe }}
    {% endcomp %}

    <section>
    <h3>Ranked Local Authorities for this Dataset</h3>
    {{ barchart_description | md | safe }}
    </br>
    {% comp 'display.figure' %}
      {{ comp.oi.chart.bar({ config: {
          data: view.total_score_barchart.rows,
          category: 'local_authority_name',
          width: 800,
          series: [
            { title: 'Weighted Score', value: 'value' }
          ],
          legend: {
            show: true,
            position: "top"
          },
          axis: {
